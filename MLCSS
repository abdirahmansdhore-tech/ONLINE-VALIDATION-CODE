import numpy as np
def MLCSS(s1,s2, t1, t2, delta): 
    """ compare sequences of event identifiers together with their respective time-stamp sequences ğ­. Namely, 
    two events with identical identifiers which are produced within a time interval longer than a threshold value ğ›¿ are not declared similar. ""
    matrix = np.zeros((len(s1), (len(s2))))
    for i in rane(len(s1)):
        for j in range(len(s2)):
            if s1(i) ==s2(j) and abs(t1(i)-t2(j))<=delta:
                if i==0 or j ==0:
                    matrix[i][j] = 1
                else:
                    matrix[i][j] = matrix[i-1][j-1]+1
            else:
                matrix[i][j] = max(matrix[i-1][j], matrix[i][j-1])
    cs = matrix[-1][-1]
    return cs
# adapted from https://github.com/giovannilugaresi/digital_twin_validator/blob/main/LCSS_delta.py
